%h1 Listing prefectures

%table
  %thead
    %tr
      %th Name
      %th
      %th
      %th

  %tbody
    - @prefectures.each do |prefecture|
      %tr
        %td= prefecture.name
        %td= link_to 'Show', prefecture
        %td= link_to 'Edit', edit_prefecture_path(prefecture)
        %td= link_to 'Destroy', prefecture, method: :delete, data: { confirm: 'Are you sure?' }

%br

= link_to 'New Prefecture', new_prefecture_path
